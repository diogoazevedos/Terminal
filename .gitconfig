[user]
  name = Diogo Azevedo
  email = diogoazevedos@gmail.com
[color]
  ui = auto
[commit]
  gpgsign = true
[credential]
  helper = osxkeychain
[push]
  default = simple
[pull]
  rebase = true
[core]
  editor = vim
  excludesfile = ~/.gitignore
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
